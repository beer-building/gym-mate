{
	"name": "@gym-mate/backend",
	"version": "1.0.0",
	"description": "This project was bootstrapped with Fastify-CLI.",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "pnpm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
		"start": "pnpm run build:ts && fastify start -l info dist/src/app.js",
		"build:ts": "tsc",
		"watch:ts": "tsc -w",
		"dev": "tsx watch src .env | pino-pretty --colorize",
		"dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js",
		"prisma:generate": "prisma generate --schema=../../prisma/schema.prisma",
		"lint": "prettier --check . && eslint . --ext .js,.ts",
		"lint:ts": "tsc -p tsconfig.json --noEmit --pretty"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^5.0.0",
		"@fastify/cors": "^8.4.1",
		"@fastify/env": "^4.3.0",
		"@fastify/jwt": "^7.2.4",
		"@fastify/sensible": "^5.0.0",
		"@fastify/swagger": "^8.12.0",
		"@fastify/swagger-ui": "^1.10.1",
		"@fastify/type-provider-typebox": "^3.5.0",
		"@gym-mate/shared-types": "workspace:*",
		"@sinclair/typebox": "^0.31.28",
		"bcryptjs": "^2.4.3",
		"dotenv": "^16.4.5",
		"fastify": "^4.0.0",
		"fastify-plugin": "^4.0.0"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/node": "^20.4.4",
		"@types/tap": "^15.0.5",
		"pino-pretty": "^11.2.2",
		"tap": "^16.1.0",
		"ts-node": "^10.4.0",
		"tsx": "^4.11.0",
		"typescript": "^5.2.2"
	}
}
