{
	"name": "@gym-mate/frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . --ext .js,.ts,.svelte",
		"lint:ts": "tsc -p tsconfig.json --noEmit --pretty",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@farfetched/core": "^0.10.6",
		"@gym-mate/shared-types": "workspace:*",
		"@gym-mate/ui": "workspace:*",
		"effector": "^22.8.7",
		"effector-storage": "^6.1.1",
		"patronum": "^1.20.0"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^2.1.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.4",
		"@types/node": "^20.9.4",
		"autoprefixer": "^10.4.14",
		"effector-logger": "^0.15.0",
		"postcss": "^8.4.24",
		"postcss-custom-media": "^10.0.2",
		"postcss-load-config": "^4.0.1",
		"postcss-nested": "^6.0.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"vite": "^4.4.2"
	},
	"type": "module"
}
